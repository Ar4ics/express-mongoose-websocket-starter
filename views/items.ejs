<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8">
    <title>EJS Engine</title>
</head>

<body>
    <div class="container">
        <table class="table table-striped">
            <thead>
                <tr>
                    <td>
                        <b>Item Name</b>
                    </td>
                    <td colspan="2">
                        <b>Action</b>
                    </td>
                </tr>
            </thead>
            <tbody>
                <% for(var i=0; i < items.length; i++) { %>
                    <tr>
                        <td>
                            <%= items[i].item %>
                        </td>
                        <td>
                            <a href="" class="btn btn-primary">Edit</a>
                        </td>
                        <td>
                            <a href="" class="btn btn-danger">Delete</a>
                        </td>
                    </tr>
                    <% } %>
            </tbody>
        </table>
    </div>
</body>

</html>